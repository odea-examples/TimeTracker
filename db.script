CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE USUARIOS(ID_USUARIO INTEGER NOT NULL PRIMARY KEY,NOMBRE VARCHAR(30),APELLIDO VARCHAR(30),PASSWORD VARCHAR(30))
CREATE MEMORY TABLE PROYECTO(ID_PROYECTO INTEGER,NOMBRE VARCHAR(50))
CREATE MEMORY TABLE ACTIVIDAD(ID_ACTIVIDAD INTEGER,NOMBRE VARCHAR(50))
CREATE MEMORY TABLE ENTRADA(ID_ENTRADA INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_PROYECTO INTEGER,ID_ACTIVIDAD INTEGER,DURACION DOUBLE,NOTA VARCHAR(300),ID_TICKET_BZ VARCHAR(50),TICKET_EXT VARCHAR(50),SISTEMA_EXT VARCHAR(50),ID_USUARIO INTEGER,FECHA TIMESTAMP)
CREATE MEMORY TABLE PROYECTO_USUARIO(ID_PROYECTO INTEGER,ID_USUARIO INTEGER)
CREATE MEMORY TABLE PROYECTO_ACTIVIDAD(ID_PROYECTO INTEGER,ID_ACTIVIDAD INTEGER)
ALTER TABLE ENTRADA ALTER COLUMN ID_ENTRADA RESTART WITH 1
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
